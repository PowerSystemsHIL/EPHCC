pv_server.epc.write()
time.sleep(.5)

#default voltage and frequency
pv_server.epc.CmdV = V_default
pv_server.epc.CmdHz = f_default
pv_server.epc.write()
time.sleep(.5)


#enable and run
pv_server.epc.CmdBits = run
pv_server.epc.write()
time.sleep(.5)

pv_server.epc.read()
print(pv_server.epc)
