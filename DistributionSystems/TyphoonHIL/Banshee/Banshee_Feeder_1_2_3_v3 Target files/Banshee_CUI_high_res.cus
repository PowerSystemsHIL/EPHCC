                <on_click_code active="True"><![CDATA[# NOTE: The code specified in this handler will be executed on button click.

# HIL Control Panel API
from typhoon.api import hil_control_panel as hil

#initialize the DMS registers before starting
hil.set_cp_input_value('User defined', 'DMS_ENGINE', dms_mcgrid_ctrl_start, 0) #Stop  microgrid controller

hil.set_cp_input_value('User defined', 'DMS_ENGINE', dms_req_to_sync, 0) #Req to sync in quiescent state
hil.wait_sec(1)

hil.set_contactor(motor_F3,swControl= True,swState= False) #open motor contactors
hil.set_contactor(motor_F1,swControl= True,swState= False) #open motor contactors

hil.set_cp_input_value('User defined', 'DMS_ENGINE', dms_disconect, 1) #disconnect F1
hil.wait_sec(1)
hil.set_cp_input_value('User defined', 'DMS_ENGINE', dms_disconect, 2) #disconnect F2
hil.wait_sec(1)
hil.set_cp_input_value('User defined', 'DMS_ENGINE', dms_disconect, 3) #disconnect F3
hil.wait_sec(1)
hil.set_cp_input_value('User defined', 'DMS_ENGINE', dms_disconect, 0) #disconnect in quiescent state
hil.wait_sec(1)
hil.set_cp_input_value('User defined', 'DMS_ENGINE', dms_req_to_sync, 5) #Req to sync all SEL POI relays
hil.wait_sec(2)
hil.set_cp_input_value('User defined', 'DMS_ENGINE', dms_req_to_sync, 0) #Req to sync in quiescent state
hil.wait_sec(1)

hil.set_cp_input_value('User defined', 'DMS_ENGINE', dms_mcgrid_ctrl_start, 1) #Start microgrid controller (this will start Gensets and ESS in grid connected mode, if everything went well)
]]></on_click_code>
